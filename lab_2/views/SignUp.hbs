	<div class="container">
		<div class="row justify-content-center">
			<div class="form align-self-center">
				<div class="nav-title text-center">Sign Up</div>

				<form action="/SignUp" method="post" id="userData">
					<div class="form-group inputs">
						<div class="ui-input">
							<label class="d-none">E-mail</label><br>
							<input type="email" name="email" placeholder="Email" value="{{normMail}}" required><br>
							<span id="postMail" class="errMess paragraph">{{errMail}}</span>
						</div>
						<div class="ui-input">
							<label class="d-none">Login</label><br>
							<input type="text" name="login" placeholder="Login" value="{{normLogin}}" required><br>
							<span id="postLogin" class="errMess paragraph">{{errLogin}}</span>

						</div>
						<div class="ui-input">
							<label class="d-none">Password</label><br>
							<input type="password" name="pass" placeholder="Password" required><br>
						</div>
					</div>
					<div class="justify-content-center d-flex">
						<button class="ui-button2 main2 mr-0" type="submit" id="sindUP"><div class="xs-title">Отправить</div></button>

					</div>

				</form>
			</div>
		</div>
	</div>